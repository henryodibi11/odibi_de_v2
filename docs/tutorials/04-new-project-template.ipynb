    "    \"config\",\n",
    "    \"data/bronze\",\n",
    "    \"data/silver\",\n",
    "    \"data/gold\",\n",
    "    \"functions/bronze\",\n",
    "    \"functions/silver\",\n",
    "    \"functions/gold\",\n",
    "    \"notebooks\",\n",
    "    \"tests\",\n",
    "    \"logs\"\n",
    "]\n",
